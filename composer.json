{
	"name": "madewithlove/semver",
	"description": "A semver checker for Packagist",
	"license": "MIT",
	"authors": [
		{
			"name": "Hannes Van De Vreken",
			"email": "vandevreken.hannes@gmail.com"
		},
		{
			"name": "Maxime Fabre",
			"email": "ehtnam6@gmail.com"
		}
	],
	"require": {
		"composer/composer": "^2.0",
		"filp/whoops": "^2.1",
		"illuminate/cache": "^8.11",
		"illuminate/filesystem": "^8.11",
		"knplabs/packagist-api": "^1.3",
		"league/container": "^3.3",
		"league/plates": "^3.1",
		"league/route": "^4.3",
		"zendframework/zend-diactoros": "^2.2"
	},
	"require-dev": {
		"mockery/mockery": "^1.3",
		"phpunit/phpunit": "^9.3",
		"symfony/var-dumper": "^5.1"
	},
	"autoload": {
		"psr-4": {
			"Semver\\": "app/Semver"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Semver\\Unit\\": "tests/unit/Semver"
		}
	},
	"minimum-stability": "stable",
	"config": {
		"sort-packages": true,
		"platform": {
			"php": "7.3.15"
		}
	}
}
